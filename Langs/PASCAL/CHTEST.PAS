uses dos;
var regs : registers;
    v    : byte;
    x    : array[1..16] of string[8];
    y    : string;
    aux  : byte;
    i,z  : integer;
begin
   x[ 1]:='00011000';
   x[ 2]:='01011010';
   x[ 3]:='01011010';
   x[ 4]:='01011010';
   x[ 5]:='00011000';
   x[ 6]:='00111100';
   x[ 7]:='01011010';
   x[ 8]:='11111111';
   x[ 9]:='01011010';
   x[10]:='00111100';
   x[11]:='00011000';
   x[12]:='00011000';
   x[13]:='01011010';
   x[14]:='01011010';
   x[15]:='01011010';
   x[16]:='00011000';
   y:='';
   aux:=0;
   for i:=1 to 16 do
   begin
      val(copy(x[i],1,1),v,z);
      aux:=2*2*2*2*2*2*2*v;
      val(copy(x[i],2,1),v,z);
      aux:=2*2*2*2*2*2*v+aux;
      val(copy(x[i],3,1),v,z);
      aux:=2*2*2*2*2*v+aux;
      val(copy(x[i],4,1),v,z);
      aux:=2*2*2*2*v+aux;
      val(copy(x[i],5,1),v,z);
      aux:=2*2*2*v+aux;
      val(copy(x[i],6,1),v,z);
      aux:=2*2*v+aux;
      val(copy(x[i],7,1),v,z);
      aux:=2*v+aux;
      val(copy(x[i],8,1),v,z);
      aux:=v+aux;
      y:=y+chr(aux);
      writeln(i:2,' - ',x[i],' - ',aux:3,' - ',y);
   end;
   regs.ah:=$11;
   regs.al:=0;
   regs.cx:=1;
   regs.dx:=255;
   regs.bh:=16;
   regs.es:=seg(y);
   regs.bp:=ofs(y);
   intr($10,regs);
   writeln;
   writeln(chr(255));
   writeln;
   readln;
end.